<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.solomarket.dao.UserDao">
    
    <insert id="registUser">
        INSERT INTO tbl_user VALUES
        (null, #{userId}, #{password}, #{userName}, #{nickName}, #{email}, #{phone}, #{birth}, #{createdAt}, #{role}, #{userImage}, #{gender}, 0, 'Y')
    </insert>


</mapper>